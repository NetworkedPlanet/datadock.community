@using DataDock.Web.ViewModels
@model List<DataDock.Web.ViewModels.RepositoryInfoViewModel>

@if (Model != null & Model.Count > 0)
{
  <select class="ui dropdown" name="repoId" id="repoId">
    @foreach (RepositoryInfoViewModel r in Model)
    {
      <option value="@r.RepoId">@r.RepoId</option>

    }
  </select>
}
else
{
  <div class="">
    No repositories found.
  </div>
}
